[
    {
        "key": "alt+q",
        "command": "calva.connect"
    },
    {
        "key": "alt+1",
        "command": "calva.loadFile"
    },
    {
        "key": "alt+2",
        "command": "calva.runCustomREPLCommand",
        "args": "(do (println \"refreshing!\") (minusthree.-dev.start/refresh))"
    },
    {
        "key": "alt+3",
        "command": "calva.runCustomREPLCommand",
        "args": "(do (println \"toggle-stop!\") (minusthree.-dev.start/toggle-stop))"
    }
]
